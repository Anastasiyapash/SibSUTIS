\par class ThreadInterrupt
\par \{
\par     public static void main(String[] args)
\par     \{
\par         StayAwake stayAwake =  new StayAwake();
\par         Thread newThread =  new Thread(new ThreadStart(stayAwake.ThreadMethod));
\par         newThread.Start();
