\par class StayAwake
\par \{
\par     private boolean sleepSwitch = false;
\par 
\par     /** @property
\par      */
\par     public void set_SleepSwitch(boolean value)
\par     \{
\par         sleepSwitch = value;
\par     \} //set_SleepSwitch
\par 
\par     public StayAwake()
\par     \{
\par     \} //StayAwake
\par 
\par     public void ThreadMethod()
\par     \{
\par         Console.WriteLine("newThread is executing ThreadMethod.");
\par         while (!(sleepSwitch)) \{
\par             // Use SpinWait instead of Sleep to demonstrate the 
\par             // effect of calling Interrupt on a running thread.
\par             Thread.SpinWait(10000000);
\par         \}
\par 
\par         try \{
\par             Console.WriteLine("newThread going to sleep.");
\par 
\par             // When newThread goes to sleep, it is immediately 
